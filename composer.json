{
    "name": "datalogix/laravel-translation",
    "description": "Laravel translation is a package the power of register paths of translations.",
    "keywords": [
        "laravel",
        "translation",
        "register",
        "paths",
        "translations"
    ],
    "homepage": "https://github.com/datalogix/laravel-translation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/translation": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.1",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.5|^10.5"
    },
    "autoload": {
        "psr-4": {
            "Datalogix\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datalogix\\Translation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datalogix\\Translation\\TranslationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
